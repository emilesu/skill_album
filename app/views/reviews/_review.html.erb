<li id="review-<%= review.id %>">
  <div class="album-review-user-avatar row">
    <%= image_tag("http://placehold.it/30x30&text=No Pic", class: "img-responsive img-circle")%>
  </div>
  <span>韵儿爸爸 ·  <%= time_ago_in_words(review.created_at) %>前</span>

  <div class="album-review-comment">
    <%= sanitize review.comment %>
  </div>
</li>
